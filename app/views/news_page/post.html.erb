<%= form_for :article, url: news_page_post_create_path do |f| %>
    <p>
      <%= f.label :title %><br>
      <%= f.text_field :headline %>
    </p>

    <p>
      <%= f.label :body %><br>
      <%= f.text_area :body %>
    </p>

    <p>
      <%= f.label :video %><br>
      <%= f.text_field :video_url %>
    </p>
    <p>
      <%= f.label :image %><br>
      <%= f.text_field :image_url %>
    </p>

    <p>
      <%= f.submit %>
    </p>
<% end %>